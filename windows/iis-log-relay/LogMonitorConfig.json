{
    "LogConfig": {
        "sources": [{
                "type": "EventLog",
                "startAtOldestRecord": true,
                "eventFormatMultiLine": false,
                "channels": [{
                        "name": "system",
                        "level": "Information"
                    },
                    {
                        "name": "application",
                        "level": "Error"
                    }
                ]
            },
            {
                "type": "File",
                "directory": "c:\\inetpub\\logs",
                "filter": "*.log",
                "includeSubdirectories": true,
                "includeFileNames": false
            }
        ]
    }
}